        });

        $("#btn-lupa").click(function(){
            var lpemail = $('#lpemail').val();

            var fd = new FormData();
            fd.append('email',lpemail);

            $("#form-loading-lupa").show();
            $("#form-loading-lupa")[0].innerHTML = "Loading...";

            $("#form-error-lupa").hide();
            $("#btn-lupa").hide();

            $.ajax({
                url: "http://tribratanews.polri.go.id/lupa",
                type: 'POST',
                data: fd,
                contentType: false,
                processData: false,
                success: function(data){
                    $("#form-loading-lupa").hide();

                    var jsonData = JSON.parse(data);
                    if(jsonData.code == "success"){
                        $("#form-success-lupa").show();
                        $("#form-success-lupa")[0].innerHTML = ""+jsonData.msg;

                        setTimeout(function() {
                            window.location.href = "http://tribratanews.polri.go.id/";
                        }, 3000);
                    }else{
                        $("#btn-lupa").show();
                        $("#form-error-lupa").show();
                        $("#form-error-lupa")[0].innerHTML = ""+jsonData.msg;

                        setTimeout(function() {
                            $("#form-error-lupa").hide();
                        }, 1000);
                    }
                }
            });
        });

        $('.tbnInfinity').jscroll({
            loadingHtml: '<div><div class="warp-list-more"><img src="http://tribratanews.polri.go.id/assets/d/images/tbn-loader.gif" alt="loading" style="width:30%"></div></div>',
            nextSelector: '.tbn-more a',
            autoTrigger : false,
            callback: function() {
                if ($('.tbn-more a').is(":visible")) {
                    var element = document.getElementById("more-index");
                    element.classList.add("hidden");
                }else{
                    var element = document.getElementById("more-index");
                    element.classList.remove("hidden");
                }
            }
        });

        $("img.lazy").lazyload();
        $("img.l-lazy").lazyload({effect: 'fadeIn'});
        $("img.r-lazy").lazyload({effect: 'fadeIn'});

        window.onscroll = function() {myFunction()};
        function myFunction() {
            var navbar = document.getElementById("navbar");
            var navbarfix = document.getElementById("nav-fixed");
            var sticky = navbar.offsetTop;
            if (window.pageYOffset >= 400) {
                navbar.classList.add("sticky");
                navbarfix.classList.add("nav-fixed");
                navbarfix.classList.remove("nav--");
            }
            if (window.pageYOffset <= 181) {
                navbar.classList.remove("sticky");
                navbarfix.classList.remove("nav-fixed");
                navbarfix.classList.add("nav--");
            }
        }

        $(".profile > a").click(function(e){
            if($(this).hasClass("buka")){
                $(this).removeClass("buka");
                $(this).parent().children("ul").stop(true,true).slideUp("normal");
            } else {
                $(this).addClass("buka");
                $(this).parent().children("ul").stop(true,true).slideDown("normal");
            }
        });

        $('#btn-lupa-password').click(function () {
            $('#masuk').modal('hide');
            document.body.style.padding = "0";
        });

    });
</script>

